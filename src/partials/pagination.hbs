<!-- start pagination -->
<div class="row">
	<div class="col-md-12">
		<div class="pagination-wrap align-center clearfix" role="navigation">
			{{#if prev}}
			<a class="newer-posts btn btn-default btn-sm" href="{{page_url prev}}"> <i class="fa fa-long-arrow-left fa-fw"></i> <span>Newer  Articles</span></a>
			{{/if}}
			<span class="page-number">Page {{page}} of {{pages}}</span>
			{{#if next}}
			<a class="older-posts btn btn-default btn-sm" href="{{page_url next}}"><span>Older  Articles</span> <i class="fa fa-long-arrow-right fa-fw"></i></a>
			{{/if}}
		</div>
	</div>
</div>
<!-- end pagination -->